db.flightData.updateOne({_id: ObjectId("5c391374a133542f6404b8ff")}, {$set:{ delayed: true}});
db.flightData.update({_id: ObjectId("5c391374a133542f6404b8ff")}, {$set:{ delayed: true, aircraft: 'Boeing 777'}});
